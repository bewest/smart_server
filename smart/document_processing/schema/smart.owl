    <!-- http://smartplatforms.org/terms#Alert -->

    <owl:Class rdf:about="http://smartplatforms.org/terms#Alert">
        <rdfs:label>Alert</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://smartplatforms.org/terms#Statement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#notes"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#notes"/>
                <owl:allValuesFrom rdf:resource="&rdfs;Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#alertLevel"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://smartplatforms.org/terms#code"/>
                        <owl:allValuesFrom rdf:resource="&spcode;AlertLevel"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#alertLevel"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#alertLevel"/>
                <owl:allValuesFrom rdf:resource="http://smartplatforms.org/terms#CodedValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Alerts are a way for an application to generate a message (about a patient record) intended for a human recipient.</rdfs:comment>
        <api:example>&lt;?xml version=&quot;1.0&quot; encoding=&quot;utf-8&quot;?&gt;
&lt;rdf:RDF
  xmlns:dcterms=&quot;http://purl.org/dc/terms/&quot;
  xmlns:rdf=&quot;http://www.w3.org/1999/02/22-rdf-syntax-ns#&quot;
  xmlns:sp=&quot;http://smartplatforms.org/terms#&quot;
  xmlns:spcode=&quot;http://smartplatforms.org/terms/codes/&quot;&gt; 

  &lt;sp:Alert&gt; 
    &lt;sp:notes&gt;Patient with T2DM is overdue for HbA1c&lt;/sp:notes&gt;
    &lt;sp:severity&gt;
      &lt;sp:CodedValue&gt;
        &lt;dcterms:title&gt;Warning&lt;/dcterms:title&gt;
        &lt;sp:code&gt;
          &lt;spcode:AlertSeverity rdf:about=&quot;http://smartplatforms.org/terms/codes/AlertSeverity#warning&quot;&gt;
            &lt;rdf:type rdf:resource=&quot;http://smartplatforms.org/terms#Code&quot; /&gt; 
            &lt;dcterms:title&gt;Warning&lt;/dcterms:title&gt; 
            &lt;sp:system&gt;http://smartplatforms.org/terms/codes/AlertSeverity#&lt;/sp:system&gt;
            &lt;dcterms:identifier&gt;warning&lt;/dcterms:identifier&gt; 
          &lt;/spcode:AlertSeverity&gt;   
        &lt;/sp:code&gt;
      &lt;/sp:CodedValue&gt;

    &lt;/sp:severity&gt; 
  &lt;/sp:Alert&gt;
&lt;/rdf:RDF&gt;</api:example>
        <api:base_path>/records/{record_id}/alerts/{alert_id}</api:base_path>
    </owl:Class>
    <owl:Axiom>
        <rdfs:comment>Message intended for a human recipient.	</rdfs:comment>
        <owl:annotatedSource rdf:resource="http://smartplatforms.org/terms#Alert"/>
        <owl:annotatedProperty rdf:resource="&rdfs;subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#notes"/>
                <owl:allValuesFrom rdf:resource="&rdfs;Literal"/>
            </owl:Restriction>
        </owl:annotatedTarget>
    </owl:Axiom>
    


